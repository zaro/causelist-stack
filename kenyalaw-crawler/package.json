{
  "name": "kenyalaw-crawler",
  "version": "0.0.1",
  "type": "module",
  "description": "This is an example of a Crawlee project.",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/util-retry": "^3.374.0",
    "crawlee": "^3.0.0",
    "object-hash": "^3.0.0"
  },
  "devDependencies": {
    "@apify/tsconfig": "^0.1.0",
    "@types/cheerio": "^0.22.35",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.0.0",
    "@types/object-hash": "^3.0.6",
    "ts-node": "^10.8.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node dist/main-causelist.js",
    "start:dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/main-causelist.ts",
    "start-cases:prod": "node dist/main-cases.js",
    "start-cases:dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/main-cases.ts",
    "process:prod": "node dist/process-crawled-data.js processData",
    "process:dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/process-crawled-data.ts  processData",
    "process-correction:prod": "node dist/process-crawled-data.js processCorrection",
    "process-correction:dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/process-crawled-data.ts  processCorrection",
    "process-cases:prod": "node dist/process-crawled-cases.js convertCasesToText",
    "process-cases:dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/process-crawled-cases.ts  convertCasesToText",
    "reprocess-files:prod": "node dist/process-crawled-data.js reprocessFiles",
    "reprocess-files:dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/process-crawled-data.ts  reprocessFiles",
    "upload-logs:prod": "node dist/process-crawled-data.js uploadLogs",
    "upload-logs:dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/process-crawled-data.ts  uploadLogs",
    "build": "tsc",
    "test": "echo \"Error: oops, the actor has no tests yet, sad!\" && exit 1"
  },
  "author": "It's not you it's me",
  "license": "ISC"
}
